<launch>
	<arg name="node_name"/>
	<let name="node_name" value="led_driver_node"/>
	<arg name="veh" description="Name of vehicle. ex: megaman"/>
    <arg name="param_file_name" default="default" description="Specify a param file. ex:megaman"/>
	<arg name="robot_type" default="duckiebot" description="Type of robot"/>

	<group>
        <push_ros_namespace namespace="$(arg veh)"/>
	    <node pkg="led_driver" exec="led_driver_node.py" name="$(arg node_name)" output="screen" required="true">
			<param from="$(find led_driver)/config/$(arg node_name)/$(arg param_file_name).yaml"/>
		</node>
	</group>

</launch>
