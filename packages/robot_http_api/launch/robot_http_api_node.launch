<?xml version="1.0"?>
<launch>
    <let name="veh" value="$(env VEH '')"/>
    <arg name="node_name" default="robot_http_api_node"/>
    <let name="param_file_name" value="$(env PARAM_FILE_NAME 'default')"/>
    <let name="robot_type" value="$(env ROBOT_TYPE '')"/>

    <group>
        <push_ros_namespace namespace="$(var veh)"/>
        <node pkg="robot_http_api" exec="$(var node_name).py" name="$(var node_name)" output="screen">
<!--          required="true"> -->
            <remap from="robot_http_api_node/estop" to="wheels_driver_node/emergency_stop"/>
            <remap from="robot_http_api_node/wheels_cmd" to="wheels_driver_node/wheels_cmd"/>
            <remap from="robot_http_api_node/shutdown_behavior" to="button_driver_node/shutdown_behavior"/>
        </node>
    </group>
</launch>
