<launch>
    <arg name="veh" description="Name of vehicle. ex: megaman"/>
    <arg name="node_name" default="wheels_driver_node"/>
    <arg name="param_file_name" default="default" description="Specify a param file. ex:megaman." />

    <node namespace="$(arg veh)" pkg="wheels_driver" exec="$(arg node_name).py" name="$(arg node_name)"
          output="screen" required="true" />

</launch>
